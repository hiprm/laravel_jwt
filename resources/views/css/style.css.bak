.online_channeling{
    background-color:#EEFEFF ;
    padding-bottom: 5%;
}

.line-bar {
    position: absolute;
    width: 50%;
    top: 50%;
    right: -50%;
}

.online_channeling_heading {
    display: flex;
    justify-content: center;
    padding-top: 15px;
    padding-bottom: 15px;
}

.online_channeling_heading h1 {
    text-align: center;
    font: normal normal normal 40px/48px Lato;
    letter-spacing: 0px;
    color: #00A9BE;
    text-transform: capitalize;
    opacity: 1;
    position: relative;
}

.online_channeling_heading img {
    float: left;
    width: 65px;
    height: 37px;
    padding-left: 12px;
    margin-top: 9px;
}

.online_channeling_element_sec {
    margin: auto;
}

.online_channeling_element {
    background: #FFFFFF 0% 0% no-repeat padding-box;
    /*box-shadow: 0px 3px 20px #00000021;*/
    /*border: 2px solid #36C1CC;*/
    border-radius: 20px;
    opacity: 1;
    width: 75%;
    height: 100%;
    margin: auto;
    padding-top: 25px;
    position: relative;
}

.online_channeling_element:hover  {
    background: #FFFFFF 0% 0% no-repeat padding-box;
    box-shadow: 0px 3px 20px #00000021;
    border: 2px solid #36C1CC;
    border-radius: 20px;
    opacity: 1;
    width: 75%;
    height: 100%;
    margin: auto;
    padding-top: 25px;
    position: relative;
}

.online_channeling_element_img {
    width: 87px;
    height: 114px;
    display: block;
    margin-left: auto;
    margin-right: auto;
}

.online_channeling_element p {
    text-align: center;
    font: normal normal bold 16px/30px Lato;
    letter-spacing: 0px;
    color: #585858;
    text-transform: capitalize;
    opacity: 1;
    padding-top: 15px;

}

body {
    background-color: #FFFFFF;
}

